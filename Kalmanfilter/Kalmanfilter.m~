function estimate =  Kalmanfilter(mu,)
global Sigma mu

Sigma_Q = diag([2 2]);                 % Measurement nosie covariance matrix
Sigma_R = diag([3 3]);                   % Process noise covariance matrix 


% Predict
[mu_bar, Sigma_bar] = KF_predict(mu,Sigma,Sigma_R);

% Update
[mu, Sigma] = update(mu_bar,Sigma_bar,Sigma_Q);


estimate = 0; 


end